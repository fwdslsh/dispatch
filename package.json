{
  "name": "dispatch",
  "version": "0.1.0",
  "description": "SvelteKit PTY + Claude Code in Docker",
  "license": "CC-BY-4.0",
  "type": "module",
  "scripts": {
    "dev": "TERMINAL_KEY=test vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite build && PORT=5170 TERMINAL_KEY=test ENABLE_TUNNEL=true node src/app.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
  },
  "dependencies": {
    "@battlefieldduck/xterm-svelte": "^2.1.0",
    "@sveltejs/adapter-node": "*",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "ansi_up": "^6.0.6",
    "augmented-ui": "^2.0.0",
    "express": "^5.1.0",
    "localtunnel": "^2.0.2",
    "node-pty": "^1.0.0",
    "playwright": "^1.55.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.36.2",
    "svelte": "^5.38.3",
    "svelte-check": "^4.3.1",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": ">=22"
  }
}
