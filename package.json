{
	"name": "@fwdslsh/dispatch",
	"version": "0.2.1",
	"description": "A web-based terminal that runs anywhere - develop, collaborate, and code with AI assistance right in your browser",
	"license": "CC-BY-4.0",
	"type": "module",
	"private": false,
	"scripts": {
		"dev": "TERMINAL_KEY='testkey12345' HOME=\"$(pwd)/.testing-home\" WORKSPACES_ROOT=\"$(pwd)/.testing-home/workspaces\" SSL_ENABLED=true vite dev --host",
		"dev:local": "TERMINAL_KEY='testkey12345' WORKSPACES_ROOT=\"$HOME/code\" SSL_ENABLED=true vite dev --host",
		"dev:server": "TERMINAL_KEY='testkey12345' SSL_ENABLED=true node src/dev-server.js",
		"dev:no-key": "TERMINAL_KEY='' SSL_ENABLED=true vite dev --host",
		"dev:tunnel": "TERMINAL_KEY=testkey12345 ENABLE_TUNNEL=true SSL_ENABLED=true vite dev --host",
		"dev:http": "TERMINAL_KEY='testkey12345' HOME=\"$(pwd)/.testing-home\" WORKSPACES_ROOT=\"$(pwd)/.testing-home/workspaces\" SSL_ENABLED=false vite dev --host",
		"dev:test": "mkdir -p /tmp/dispatch-test-home /tmp/dispatch-test-workspaces && PORT=7173 TERMINAL_KEY='test-automation-key-12345' HOME=/tmp/dispatch-test-home WORKSPACES_ROOT=/tmp/dispatch-test-workspaces SSL_ENABLED=false vite dev --host --port 7173",
		"build": "vite build",
		"build:strict": "npm run check && vite build",
		"preview": "vite preview",
		"start": "npm run build && PORT=5170 TERMINAL_KEY=testkey12345 ENABLE_TUNNEL=true SSL_ENABLED=true node src/app.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"test:original": "USE_SIMPLIFIED_SESSIONS=false npm run test:unit -- --run",
		"test:simplified": "npm run test:unit -- --run",
		"test:core": "HOME=\\\"$(pwd)\\\" npm run -s test:managers",
		"test:e2e": "node run-e2e-tests.js",
		"test:e2e:headed": "node run-e2e-tests.js --headed",
		"test:e2e:debug": "npx playwright test --debug",
		"test:e2e:ui": "npx playwright test --ui",
		"test:e2e:report": "npx playwright show-report",
		"test:ui": "node run-ui-tests.js",
		"test:ui:headed": "node run-ui-tests.js --headed",
		"test:ui:debug": "node run-ui-tests.js --debug",
		"test:ui:ui": "node run-ui-tests.js --ui",
		"playwright:install": "npx playwright install",
		"docker:dev": "node bin/cli.js start -k test --projects ~/code/ --home ~/ --build",
		"docker:start": "node bin/cli.js start -k test --projects ~/code/ --home ~/",
		"docker:stop": "node bin/cli.js stop",
		"css:clean": "node scripts/remove-unused-css.js",
		"css:clean:dry-run": "node scripts/remove-unused-css.js --dry-run --verbose",
		"css:clean:backup": "node scripts/remove-unused-css.js --backup --verbose",
		"css:map": "node scripts/map-css-usage.js",
		"css:map:verbose": "node scripts/map-css-usage.js --verbose",
		"css:vars": "node scripts/map-css-variables.js",
		"css:vars:verbose": "node scripts/map-css-variables.js --verbose",
		"css:vars:unused": "node scripts/map-css-variables.js --unused-only",
		"css:complexity": "node scripts/analyze-css-complexity.js",
		"css:complexity:strict": "node scripts/analyze-css-complexity.js --threshold 5",
		"css:analyze-components": "node scripts/analyze-component-styles.js",
		"css:analyze-components:verbose": "node scripts/analyze-component-styles.js --verbose",
		"css:duplicates": "node scripts/detect-duplicate-css.js",
		"css:duplicates:strict": "node scripts/detect-duplicate-css.js --threshold 95",
		"css:migrate": "node scripts/migrate-to-component-styles.js",
		"css:migrate:component": "node scripts/migrate-to-component-styles.js --component",
		"css:tokens": "node scripts/audit-css-tokens.js",
		"css:tokens:verbose": "node scripts/audit-css-tokens.js --verbose"
	},
	"bin": {
		"dispatch": "./bin/dispatch"
	},
	"dependencies": {
		"@anthropic-ai/claude-code": "^2.0.0",
		"@battlefieldduck/xterm-svelte": "^2.1.1",
		"@floating-ui/dom": "^1.7.4",
		"@sveltejs/adapter-node": "^5.3.3",
		"@xterm/addon-fit": "^0.10.0",
		"@xterm/xterm": "^5.5.0",
		"ansi_up": "^6.0.6",
		"augmented-ui": "^2.0.0",
		"bcrypt": "^6.0.0",
		"better-sqlite3": "^12.4.1",
		"commander": "^12.1.0",
		"express": "^5.1.0",
		"hammerjs": "^2.0.8",
		"localtunnel": "^2.0.2",
		"markdown-it": "^14.1.0",
		"marked": "^16.4.0",
		"node-forge": "^1.3.1",
		"node-pty": "^1.0.0",
		"open": "^10.2.0",
		"prismjs": "^1.30.0",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"sqlite3": "^5.1.7",
		"sv-window-manager": "^0.2.0",
		"svelte-virtual-list": "^3.0.1"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.56.0",
		"@sveltejs/kit": "^2.46.4",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/browser": "^3.2.3",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"jsdom": "^26.1.0",
		"node-fetch": "^3.3.2",
		"playwright": "^1.53.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"supertest": "^7.1.4",
		"svelte": "^5.39.11",
		"svelte-check": "^4.3.3",
		"vite": "^7.1.9",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^0.1.0"
	},
	"engines": {
		"node": ">=22"
	}
}
